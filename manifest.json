{
  "manifest_version": 2,
  "name": "sessio",
  "version": "0.0.1",
  "background": {
    "scripts": ["background/background.js",
        "background/words.js",
        "libs/lodash.js",
        "background/sessionCreator.js"
    ]
  },

  "content_scripts": [
    {
    "matches" : ["http://*/*", "https://*/*"],
    "js": ["content/content.js"]
    }
    ],
  "description": "Browser history session organizer",
  "icons": {},
  "permissions": [
		"tabs", "http://*/*", "storage", "unlimitedStorage", "history"
	],
  "browser_action" : {}
}
